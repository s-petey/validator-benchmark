{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.json",
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "noEmit": false,
    "strictNullChecks": true,
    "plugins": [{ "transform": "typia/lib/transform" }],

    "lib": ["es2022"]
  }
}
